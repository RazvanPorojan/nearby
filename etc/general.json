{

	"name": "nearby",

	"static": {
		"path": "/srv",
    "index": false,
		"passthrough": true
	},

	"engine": {
    "views": {
      "path": "/usr/views",
      "engine": "jade"
    },
    "builder": {
      "entry": "/app/index.js",
      "output": "/srv/javascripts/build.js",
      "extensions": [
        ".js", ".json"
      ]
    }
	}

}
